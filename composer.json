{
  "name": "ueef/pharus",
  "type": "library",
  "license": "MIT",
  "description": "The Pharus is a simple request dispatcher",
  "config": {
    "vendor-dir": "var/vendor"
  },
  "require": {
    "php": ">=7.4",
    "ueef/pheseus": "^0.0"
  },
  "autoload": {
    "psr-4": {
      "Ueef\\Pharus\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Ueef\\Pharus\\Tests\\": "tests"
    }
  }
}
